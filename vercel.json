{
    "version": 2,
    "builds": [
        {
            "src": "server.ts",
            "use": "@vercel/node",
            "config": {
                "zeroConfig": true,
                "buildCommand": "tsc"
            }
        },
        {
            "src": "vite.config.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/server.ts"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}
